    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.dk.boosted.DisplaySummoners"
    android:weightSum="1"
    android:orientation="vertical"
    android:rowCount="10"
    android:columnCount="5">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/summoner1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:minHeight="40dp"
        android:minWidth="40dp"
        android:layout_column="0"
        android:layout_row="0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/summName1"
        android:layout_row="0"
        android:layout_column="1"
        android:textIsSelectable="false"
        android:textSize="15dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/summoner2"
        android:minHeight="40dp"
        android:minWidth="40dp"
        android:layout_below="@+id/summoner1"
        android:layout_alignParentStart="true"
        android:layout_column="0"
        android:layout_row="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/summName2"
        android:layout_row="1"
        android:layout_column="1"
        android:textSize="15dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/summoner3"
        android:minHeight="40dp"
        android:minWidth="40dp"
        android:layout_below="@+id/summoner2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="7dp"
        android:layout_row="2"
        android:layout_column="0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/summName3"
        android:layout_row="2"
        android:layout_column="1"
        android:textSize="15dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/summoner4"
        android:minHeight="40dp"
        android:minWidth="40dp"
        android:layout_below="@+id/summoner3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="7dp"
        android:layout_row="3"
        android:layout_column="0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/summName4"
        android:layout_row="3"
        android:layout_column="1"
        android:textSize="15dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/summoner5"
        android:minHeight="40dp"
        android:minWidth="40dp"
        android:layout_below="@+id/summoner4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="7dp"
        android:layout_row="4"
        android:layout_column="0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/summName5"
        android:layout_row="4"
        android:layout_column="1"
        android:textSize="15dp" />

